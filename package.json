{
  "name": "yarn-pnp-angular",
  "version": "0.0.0",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "ng": "pnpify ng",
    "start": "pnpify ng serve --ssl --host 0.0.0.0 --port 8200 --ssl-key ./localhost.nopass.key.pem  --ssl-cert ./localhost.cert.pem",
    "build": "pnpify ng build",
    "watch": "pnpify ng build --watch --configuration development",
    "test": "pnpify ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/pwa": "^14.0.3",
    "@angular/router": "^14.0.0",
    "@angular/service-worker": "^14.0.3",
    "@yarnpkg/pnpify": "^4.0.0-rc.9",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.0.0",
    "@types/jasmine": "~4.0.0",
    "@types/node": "~16",
    "eslint": "^8.18.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.9.1",
    "typescript": "~4.7.2"
  },
  "dependenciesMeta": {
    "@angular/service-worker@14.0.3": {
      "unplugged": true
    }
  }
